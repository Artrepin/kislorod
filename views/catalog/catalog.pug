extends ../layouts/layout.pug

block main
    #catalogvue.main-wrapper
        .main-screen.main-screen_catalog.bg_dark-blue.color_white
            .main
                include ../include/header.pug
                .catalog-filter
                    form(action='#')
                        .catalog-filter__row.row
                            .col-sm-3.col-min-6.mt10.js-slider-wrapper(v-if="filters.price")
                                label.ff_trajan.font_size18(for='price') Цена:
                                .input-box.mt10
                                    input.catalog-filter__mask.catalog-filter__input.input-text.input-text_border.js-slider-mask(type='number', v-model="selected.price")
                                    input#price.catalog-filter__input.input-text.input-text_border.js-slider-options(type='text', v-model="selected.price + ' млн'", :data-value='filters.price.max', :data-min='filters.price.min', :data-max='filters.price.max', data-step='1', data-meas='млн', disabled='')
                                    .catalog-filter__slider.js-slider
                                .catalog-filter__line.mt10
                                    .font_size14.fade05
                                        span.js-slider-min {{ filters.price.min }}
                                        |  млн
                                    .font_size14.fade05
                                        span.js-slider-max {{ filters.price.max }}
                                        |  млн
                            .col-sm-3.col-min-6.mt10.js-slider-wrapper(v-if="filters.area")
                                label.ff_trajan.font_size18(for='square') Площадь квартиры:
                                .input-box.mt10
                                    input.catalog-filter__mask.catalog-filter__input.input-text.input-text_border.js-slider-mask(type='number', v-model="selected.area")
                                    input#square.catalog-filter__input.input-text.input-text_border.js-slider-options(type='text', v-model="selected.area + ' м2'", :data-value='filters.area.max', :data-min='filters.area.min', :data-max='filters.area.max', data-step='1', data-meas='м2', disabled='')
                                    .catalog-filter__slider.js-slider
