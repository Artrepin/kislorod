                                    .font_size14.fade05
                                        span.js-slider-min {{ filters.area.min }}
                                        |  м2
                                    .font_size14.fade05
                                        span.js-slider-max {{ filters.area.max }}
                                        |  м2
                            .col-sm-3.col-min-6.mt10.js-slider-wrapper(v-if="filters.floor")
                                label.ff_trajan.font_size18(for='floor') Этажность:
                                .input-box.mt10
                                    input.catalog-filter__mask.catalog-filter__input.input-text.input-text_border.js-slider-mask(type='number', v-model="selected.floor")
                                    input#floor.catalog-filter__input.input-text.input-text_border.js-slider-options(type='text', v-model="selected.floor+ ' этаж'", :data-value='filters.floor.max', :data-min='filters.floor.min', :data-max='filters.floor.max', data-step='1', data-meas='этаж', disabled='')
